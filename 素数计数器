#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<math.h>

int func1(int x, int y);
int func2(int m);

int main()
{
	int a, b, d, count = 0;
	scanf("%d %d", &a, &b);

	int c = func1(a, b);
	if (a == c) {
		d = b;
	}
	else {
		d = a;
	}
	for (int i = c; i <= d; i++) {
		if(func2(i) == 0){
			count++;
		}
	}

	printf("%d", count);

	return 0;
}

int func1(int x, int y) {
	int m = (x < y? x: y);
	return m;
}

int func2(int m) {
	if (m == 2) {
		return 0;
	}
	else if (m % 2 == 0 || m < 2) {
		return 1;
	}
	else {
		for (int n = 3; n <= sqrt(m); n += 2) {
			if (m % n == 0) {
				return 1;
			}
		}
		return 0;
	}
}
