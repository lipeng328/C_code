#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>

int main()
{
	int grade;
	printf("请输入成绩：");
	scanf("%d", &grade);

	if (grade < 0 || grade > 100) {
		printf("错误");
		return 1;
	}
	else if (grade >= 90) {
		printf("成绩优秀");
	}
	else if (grade >= 80) {
		printf("成绩良好");
	}
	else if (grade >= 70) {
		printf("成绩中等");
	}
	else if (grade > 60) {
		printf("成绩及格");
	}
	else {
		printf("成绩不及格");
	}
	return 0;
}
