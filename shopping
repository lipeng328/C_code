#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<math.h>

int main()
{
    int a, b;
    scanf("%d %d", &a, &b);

    if (a < 0 || a > pow(10, 4) || b < 0 || b > 9) {
        printf("错误");
    }

    else {
        int num;
        num = floor((a * 10) / 19);
            printf("%d", num);
    }

    return 0;
}
