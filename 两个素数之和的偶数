#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>

int a, b;

int func();

int main() 
{
	int i;
	for (i = 0; i <= 10000; i += 2) {
		for (a = 2; a + a < i; a++) {
			if (func(a) ==  0) {
				b = i - a;
				if (func(b) == 0) {
					printf("%d = %d + %d\t", i, a, b);
				}
			}
		}
	}
	return 0;
}

int func(int c)
{
	int count = 0;

	if (c == 2) {
		return 0;
	}
	else if (c % 2 == 0 || c <= 1) {
		return 1;
	}
	else {
		for (int j = 3; j * j <= c; j += 2) {
			if (c % j == 0) {
				count++;
			}
		}

		if (count == 0) {
			return 0;
		}
		else {
			return 1;
		}
	}
}
