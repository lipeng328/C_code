#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>

int main()
{
	int grade;
	printf("请输入成绩：");
	scanf("%d",&grade);

	if (grade < 0 || grade > 100) {
		printf("成绩错误！");
		return 1;
	}
	else {
		switch (grade / 10) {
		case 10:
		case 9:
			printf("成绩优秀\n");
			break;
		case 8:
			printf("成绩良好\n");
			break;
		case 7:
			printf("成绩中等\n");
			break;
		case 6:
			printf("成绩及格\n");
			break;
		default:
			printf("成绩不及格\n");
		}
	}
	return 0;
}
