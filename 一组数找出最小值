#include<stdio.h>

int main()
{
    int num, i;
    scanf("%d", &num);
    int arr[1000] = {0};
    for(i = 0; i < num ; i++){
        scanf("%d", &arr[i]);
    }

    for(int j = 0; j < 1000； j++){
        for(i = 0; i < num； i++){
            if(j == arr[i]){
                printf("%d", arr[i]);
                return 0;
            }
        }
    }
    return 0;
}
