#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>

int main()
{
	float score;
	printf("请输入成绩：");
	scanf("%f",&score);

	if (score < 0 || score > 100) {
		printf("错误!请重新输入！");
		return 1;
	}

	else {
		if (score >= 90 && score <= 100) {
			printf("优");
		}
		if (score >= 80 && score < 90) {
			printf("良");
		}
		if (score >= 70 && score < 80) {
			printf("中");
		}
		if (score >= 60 && score < 70) {
			printf("及格");
		}
		if (score < 60 && score >= 0){
			printf("不及格");
		}
	}

	return 0;
}
