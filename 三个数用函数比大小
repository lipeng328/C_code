#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>

int main()
{
	int arr[] = { 1 , 2 , 3 };
	printf("请输入三个数比大小：");
	scanf("%d %d",&arr[0],&arr[1]);

	if (arr[0] > 10000 || arr[0] < -10000 || arr[1] < -10000 || arr[1] > 10000 || arr[2] < -10000 || arr[2] > 10000) {
		printf("输入不合法！");
		return 0;
	}
	else {
		int m = Max(arr[0], arr[1],arr[2]);
		printf("%d",m);
	}
	return 0;
}

int Max(int x, int y , int z)
{
	if (x > y && x > y) {
		return x;
	}
	else if(y > x && y > z){
		return y;
	}
	else {
		return z;
	}
}
